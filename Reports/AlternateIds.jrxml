	<queryString>
		<![CDATA[SELECT
     alt.identifier AS patientId,
     alt.datechanged AS dateChanged
FROM
     alternatepatientidentifier alt
INNER JOIN patient p on p.id = alt.patient
INNER JOIN patientidentifier pi on pi.patient_id = p.id
WHERE
     pi.value = $P{patientId}
ORDER BY
     alt.datechanged DESC]]>
	</queryString>
